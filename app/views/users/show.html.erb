  <div class="col-md-4 col-md-offset-4 cntr">
    <%= gravatar_for @user, size: 150 %>
  </div>
</div>
<h4 class="cntr">Checkout <%= @user.username %>'s articles</h4>
<%= render 'articles/article', obj: @user.articles %>